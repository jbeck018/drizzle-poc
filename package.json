{
	"name": "drizzle-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bun ./server/server",
		"build": "remix vite:build",
		"start": "remix-serve ./build/server/index.js",
		"typecheck": "tsc",
		"server:start": "bun ./server/server",
		"preview": "bunx --bun vite preview",
		"codegen": "graphql-codegen --config codegen.ts",
		"start:db": "docker-compose up -d",
		"seed": "bun ./db/seeding/seed.ts",
		"generate": "drizzle-kit generate --config ./db/drizzle.config.ts",
		"migrate": "drizzle-kit migrate --config ./db/drizzle.config.ts",
		"studio": "drizzle-kit studio --config=./db/drizzle.config.ts --port 4983 --verbose",
		"push": "drizzle-kit push --config ./db/drizzle.config.ts",
		"up": "drizzle-kit up --config ./db/drizzle.config.ts",
		"start:stripe": "stripe listen --forward-to localhost:3000/api/webhook"
	},
